{
  "name": "spotishush",
  "version": "2018.6.19",
  "license": "MIT",
  "author": "guihkx",
  "dependencies": {
    "webextension-polyfill": "^0.5.0"
  },
  "devDependencies": {
    "standard": "^14.3.1",
    "web-ext": "^3.2.1"
  },
  "scripts": {
    "build": "cp ./node_modules/webextension-polyfill/dist/browser-polyfill.min.js ./src/ && web-ext build --overwrite-dest --source-dir ./src/",
    "standard:check": "standard --verbose ./src/**/*.js",
    "standard:fix": "standard --fix --verbose ./src/**/.js",
    "start:chrome": "web-ext run --browser-console --source-dir ./src/ --target chromium --url https://accounts.spotify.com/en/login?continue=https://open.spotify.com/",
    "start:firefox": "web-ext run --browser-console --source-dir ./src/ --target firefox-desktop --url https://accounts.spotify.com/en/login?continue=https://open.spotify.com/"
  }
}